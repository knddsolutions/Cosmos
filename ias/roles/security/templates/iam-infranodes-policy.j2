{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": "ec2:*",
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::kd-{{ aws_account_id }}-docker-registry-{{ region }}-{{ tag_environment | lower }}",
                "arn:aws:s3:::kd-{{ aws_account_id }}-docker-registry-{{ region }}-{{ tag_environment | lower }}/*"
            ]
        }
    ]
}
